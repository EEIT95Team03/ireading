<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class name="org.iii.eeit9503.ireading.model.JoinListBean" table="JoinList">

		<id name="EventID" column="EventID">
			<generator class="identity"/>
		</id>

		<property name="MemberID" column="MemberID" />

		<set name="events" table="Event" lazy="false" cascade="all"  inverse="true"  order-by="EventID asc">			
			<key>
				<column name="EventID" />
			</key>
			<one-to-many class="org.iii.eeit9503.ireading.model.EventBean" />
		</set>

	</class>
</hibernate-mapping>

