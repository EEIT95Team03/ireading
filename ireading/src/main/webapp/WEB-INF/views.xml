	<bean id="Event.update"
		class="org.springframework.web.servlet.view.InternalResourceView">
		<property name="url" value="/pages/event/EventAll.jsp"></property>
	</bean>

<!-- bookcase -->
	<bean id="bookcase.insert"
		class="org.springframework.web.servlet.view.InternalResourceView">
